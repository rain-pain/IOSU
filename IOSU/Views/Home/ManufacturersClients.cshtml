@model List<string>

<div class="text-center">
    <h4>Общий список производителей и заказчиков</h4>
</div>

<table class="table table-dark">
    <thead>
        <tr>
            <th>
                @Html.DisplayName("Name")
            </th>
            <th>
                @Html.DisplayName("Принадлежность")
            </th>
        </tr>
    </thead>
    <tbody>
@for(int i = 0; i < Model.Count(); i++) {
        <tr>
            <td>
                @Model[i]
            </td>
            <td>
                @ViewBag.Mc[i]
            </td>
        </tr>
}
    </tbody>
</table>
